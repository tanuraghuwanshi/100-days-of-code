// Q12: Write a program to input an integer and check whether it is positive, negative or zero using nested ifâ€“else.
#include <stdio.h>
int main() {
int num;
printf("Enter an integer : ");
scanf("%d", &num);
if(num > 0) {
printf("Number: %d\tIt is Positive\n", num);
} else {
if(num < 0) {
printf("Number: %d\tIt is Negative\n", num);
} else {
printf("Number: %d\tIt is Zero\n", num);
}
}
return 0;
}
