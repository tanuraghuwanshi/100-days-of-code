// Q6: Write a program to swap two numbers using a third variable.
#include <stdio.h>
int main() {
int num1, num2, temp;
printf("First Number  : ");
scanf("%d", &num1);
printf("Second Number : ");
scanf("%d", &num2);
temp = num1;
num1 = num2;
num2 = temp;
printf("After Swapping -> Num1: %d\tNum2: %d\n", num1, num2);
return 0;
}
