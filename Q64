// Q64: Find the digit that occurs the most times in an integer number.
#include <stdio.h>
int main() {
int num, temp, digit;
int freq[10] = {0};
scanf("%d", &num);
if(num < 0) num = -num;
temp = num;
while(temp != 0) {
digit = temp % 10;
freq[digit]++;
temp /= 10;
}
int maxFreq = 0, maxDigit = 0;
for(int i = 0; i < 10; i++) {
if(freq[i] > maxFreq) {
maxFreq = freq[i];
maxDigit = i;
}
}
printf("%d\n", maxDigit);
return 0;
}
