Q100: Print all sub-strings of a string.
#include <stdio.h>
#include <string.h>
int main() {
char str[1000];
int len;
printf("Enter a string: ");
fgets(str, sizeof(str), stdin);
len = strlen(str);
if (str[len - 1] == '\n') 
str[len - 1] = '\0';
len = strlen(str);
printf("All substrings:\n");
for (int start = 0; start < len; start++) {
for (int end = start; end < len; end++) {
for (int k = start; k <= end; k++)
printf("%c", str[k]);
printf("\n");
}
}
return 0;
}
