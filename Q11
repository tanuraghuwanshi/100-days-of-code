// Q11: Write a program to input an integer and check whether it is even or odd using ifâ€“else.
#include <stdio.h>
int main() {
int num;
printf("Enter an integer : ");
scanf("%d", &num);
if(num % 2 == 0) {
printf("Number: %d\tIt is Even\n", num);
} else {
printf("Number: %d\tIt is Odd\n", num);
}
return 0;
}
