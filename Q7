// Q7: Write a program to swap two numbers without using a third variable.
#include <stdio.h>
int main() {
int num1, num2;
printf("First Number  : ");
scanf("%d", &num1);
printf("Second Number : ");
scanf("%d", &num2);
num1 = num1 + num2;
num2 = num1 - num2;
num1 = num1 - num2;
printf("After Swapping -> Num1: %d\tNum2: %d\n", num1, num2);
return 0;
}
